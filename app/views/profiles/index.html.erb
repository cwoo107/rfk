
<div class="container" id="white">
  <div class="row">
    <div class="col-md-12 text-center">

      <% if @profile == nil %>
        &nbsp; <p />
        <h2>Click the link to get started</h2>
        &nbsp; <p />
        <%= link_to('Start Your Application', new_profile_path, :class=>"btn btn-lg btn-info") %>
      <%else %>


        <p id="notice"><%= notice %></p>

        <h1>Profile</h1>

        <table>
          <thead>
          <tr>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Sex</th>
            <th>Birthdate</th>
            <th>Married</th>
            <th>Address</th>
            <th>Address2</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Phone</th>
            <th>Email</th>
            <th colspan="3"></th>
          </tr>
          </thead>

          <tbody>
          
            <tr>
              <td><%= @profile.firstname %></td>
              <td><%= @profile.lastname %></td>
              <td><%= @profile.sex %></td>
              <td><%= @profile.birthdate %></td>
              <td><%= @profile.married %></td>
              <td><%= @profile.address %></td>
              <td><%= @profile.address2 %></td>
              <td><%= @profile.city %></td>
              <td><%= @profile.state %></td>
              <td><%= @profile.zip %></td>
              <td><%= @profile.phone %></td>
              <td><%= @profile.user.email %></td>
              <td><%= link_to 'Show', @profile %></td>

            </tr>
          
          </tbody>
        </table>

        <br>


        <div id="hello"></div>
      <%end %>
    </div>
  </div>
</div>